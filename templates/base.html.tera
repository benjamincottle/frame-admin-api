      <div>
        <a href="{{ url_for(name='login') }}"><button type="button">Login</button></a>
      </div>
{% endif %}
    </section>

    <section class="layout_column">
      <div>
        {% block content %} {% endblock %}
      </div>
    </section>
    </div>
    {% block scripts %}{% endblock %}
  </body>
</html>
